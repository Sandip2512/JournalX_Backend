{
    "version": 2,
    "builds": [
        {
            "src": "app/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/main.py"
        }
    ],
    "crons": [
        {
            "path": "/api/calendar/cron/sync",
            "schedule": "0 * * * *"
        }
    ]
}